{
  "name": "osu-standard-stable",
  "version": "4.0.4",
  "description": "osu!stable version of osu!standard ruleset based on osu!lazer source code.",
  "exports": {
    "import": "./lib/index.mjs",
    "require": "./lib/index.cjs"
  },
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c && npm run format",
    "test": "jest --verbose",
    "fix": "eslint --fix ./src",
    "format": "eslint --fix ./lib/** --no-ignore",
    "prepublishOnly": "npm run build && npm run test",
    "docs": "npx typedoc src/index.ts"
  },
  "keywords": [
    "osu",
    "osu!lazer",
    "osu!standard",
    "osu!std"
  ],
  "author": "Kionell",
  "repository": {
    "type": "git",
    "url": "https://github.com/kionell/osu-standard-stable"
  },
  "license": "MIT",
  "peerDependencies": {
    "osu-classes": "^3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "jest-esm-transformer": "^1.0.0",
    "osu-parsers": "^4.1.6",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-node-externals": "^7.0.1",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3"
  }
}
